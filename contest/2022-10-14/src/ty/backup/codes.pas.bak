procedure init;
var
i,x,y:longint;
begin
readln(n);
for i:= 1 to n do readln(s[i]);
end;

function hash(t:ansistring):longint;
var
k,s,i,x,y:longint;
begin
s:=0;
k:=length(t);if k>1000 then k:=1000;
for i:= 1 to length(t) do
 if t[i]='1' then s:=(s+num[i]) mod p
 else s:=(s+num[i]+num[i]) mod p;
exit(s);
end;

procedure dfs(t:ansistring;k:longint);
var
i,x,y,len:longint;
begin
len:=length(t);
//writeln(t,' ',k);
//if len>k then writeln('-_-)');
if chk then exit;
if len>k then exit;
if len=k then
  begin
  x:=hash(t);
  if b[x] then
    begin
    if (words = '') or (words<>'')and(t<words) then words:=t;
    cln:=k;
    chk:=true;
    end;
  if not b[x] then b[x]:=true;
  exit;
  end;
for i:= 1 to n do
 dfs(t+s[i],k);
end;

procedure main;
var
i,j,x,y:longint;
begin
chk:=false;
for i:= 1 to maxlongint do
  begin
  //writeln('i=',i);
  dfs('',i);
  if chk then break;
  end;
writeln(cln);
writeln(words);
end;

begin
assign(input,'codes.in');reset(input);
assign(output,'codes.out');rewrite(output);
init;
main;
close(input);close(output);
end.


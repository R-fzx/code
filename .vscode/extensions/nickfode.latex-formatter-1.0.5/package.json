{
	"name": "latex-formatter",
	"displayName": "latex-formatter",
	"description": "Simple LaTex formatter.",
	"icon": "img/icon.svg",
	"version": "1.0.5",
	"publisher": "nickfode",
	"repository": {
		"type": "git",
		"url": "https://github.com/nfode/latex-formatter"
	},
	"engines": {
		"vscode": "^1.10.0"
	},
	"categories": [
		"Other",
		"Formatters"
	],
	"activationEvents": [
		"onLanguage:latex"
	],
	"main": "./out/src/extension",
	"contributes": {
		"languages": [
			{
				"id": "latex",
				"aliases": [
					"tex"
				]
			}
		],
		"capabilities": {
			"documentFormattingProvider": "true"
		},
		"commands": [
			{
				"command": "extension.sayHello",
				"title": "Hello World"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^2.0.3",
		"vscode": "^1.0.0",
		"mocha": "^2.3.3",
		"@types/node": "^6.0.40",
		"@types/mocha": "^2.2.32"
	},
	"__metadata": {
		"id": "ea8da575-2c65-42a9-b0e2-68bf3e3bce0d",
		"publisherId": "a4fc69fc-54ca-42d3-94e5-55d123722de6",
		"publisherDisplayName": "Nick Fode",
		"installedTimestamp": 1636366309705
	}
}